{
  "compilerOptions": {
    "strict": false,
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "target": "ES5",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"],
      "@-react/*": ["src/pages/react/*"],
      "@-vue/*": ["src/pages/vue/*"]
    }
  }
}